# Copy to .env and adjust for your environment.
# All settings are prefixed with FOLD_*

# Where job directories will be created
FOLD_BASE_DIR=/af3_raid0/web_jobs

# Path to the AlphaFold3 runner wrapper script/binary
FOLD_AF3_RUN_CMD=/usr/local/bin/af3run

# Path to a logo image shown in the sidebar (relative or absolute)
FOLD_LOGO_FILE=logo.png
